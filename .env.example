# ManeYantra Environment Configuration
# ⚠️  DO NOT use these values in production!
# Run: python scripts/setup_secrets.py to generate secure secrets

# RabbitMQ Broker Configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=CHANGE_ME_GENERATE_SECURE_USERNAME
RABBITMQ_PASSWORD=CHANGE_ME_GENERATE_SECURE_PASSWORD
RABBITMQ_VHOST=/

# Secret Key (for encryption, tokens, etc.)
SECRET_KEY=CHANGE_ME_GENERATE_SECURE_SECRET_KEY

# Eufy Credentials (Optional - fill in if using Eufy devices)
EUFY_USERNAME=
EUFY_PASSWORD=
EUFY_COUNTRY=US

# TP-Link Kasa Configuration (Optional)
# TPLINK_USERNAME=
# TPLINK_PASSWORD=

# System Configuration
LOG_LEVEL=INFO
DATA_DIR=./data
LOGS_DIR=./logs

# Development/Production Mode
ENVIRONMENT=production
